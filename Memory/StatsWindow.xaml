<Window x:Class="Memory.StatsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="StatsWindow" Height="300" Width="300" Activated="Window_Activated">
    <Grid>
        <DataGrid x:Name="FivesGrid" Height="180" Margin="64,41,0,0" VerticalAlignment="Top" Width="156">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Name" Binding="{Binding name}"/>
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="Button" HorizontalAlignment="Left" Height="92" Margin="13,74,0,0" VerticalAlignment="Top" Width="58" Click="Button_Click"/>

    </Grid>
</Window>
